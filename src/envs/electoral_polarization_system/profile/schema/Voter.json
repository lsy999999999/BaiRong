    "evaluation_results": {
        "type": "dict",
        "default": {},
        "private": true,
        "sampling": "default"
    },
    "current_attitudes": {
        "type": "dict",
        "default": {},
        "private": true,
        "sampling": "default"
    },
    "updated_attitudes": {
        "type": "dict",
        "default": {},
        "private": true,
        "sampling": "default"
    },
    "id": {
        "type": "int",
        "default": "0",
        "private": true,
        "sampling": "default",
        "description": "Unique id of agent"
    }
