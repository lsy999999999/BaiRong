{
    "name": {
        "type": "str",
        "default": "Witness",
        "private": false,
        "sampling": "default"
    },
    "testimony_content": {
        "type": "str",
        "default": "awaiting_input",
        "private": true,
        "sampling": "default"
    },
    "prepared_testimony": {
        "type": "str",
        "default": "ready",
        "private": true,
        "sampling": "default"
    },
    "testimony_given": {
        "type": "str",
        "default": "inactive",
        "private": true,
        "sampling": "default"
    },
    "credibility": {
        "type": "float",
        "default": 75.0,
