{
    "name": {
        "type": "str",
        "default": "DefenseLawyer",
        "private": false,
        "sampling": "llm",
        "description": "The name of the agent representing the defense lawyer in the simulation."
    },
    "defense_strategy": {
        "type": "str",
        "default": "awaiting_input",
        "private": true,
        "sampling": "default"
    },
    "prepared_defense": {
        "type": "str",
        "default": "ready",
        "private": true,
        "sampling": "default"
    },
    "plea_terms": {
        "type": "str",
        "default": "negotiation_pending",
        "private": true,
        "sampling": "default"
    },
    "negotiated_plea": {
        "type": "str",
        "default": "unconfirmed",
        "private": true,
        "sampling": "default"
    },
    "case_evaluation": {
        "type": "str",
        "default": "initial_review",
        "private": true,
        "sampling": "default"
    },
    "defense_strategy_options": {
        "type": "list",
        "default": [
            "aggressive",
            "cautious",
            "evidence_based"
        ],
        "private": false,
        "sampling": "random",
        "choices": [
            "aggressive",
            "cautious",
            "evidence_based",
            "plea_focus"
