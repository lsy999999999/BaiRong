    "public_attention": {
        "type": "int",
        "default": 50,
        "private": false,
        "sampling": "default"
    },
    "precedent_strength": {
        "type": "float",
        "default": 0.75,
        "private": false,
        "sampling": "default"
    },
    "procedural_rule_strictness": {
        "type": "int",
        "default": 70,
        "private": false,
        "sampling": "default"
    },
    "id": {
        "type": "str",
        "default": "0",
        "private": true,
        "sampling": "default",
        "description": "Unique id of agent"
    }
