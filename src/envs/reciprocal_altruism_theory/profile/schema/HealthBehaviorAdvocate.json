        "type": "str",
        "default": "HealthBehaviorAdvocate",
        "private": false,
        "sampling": "llm",
        "description": "The agent's role in promoting health behaviors"
    },
    "behavior_type": {
        "type": "str",
        "default": "exercise",
        "private": false,
        "sampling": "llm",
        "description": "Type of health behavior advocated by the agent"
    },
    "intensity": {
