    "political_orientation": {
        "type": "str",
        "default": "Conservative",
        "private": false,
        "sampling": "random",
        "choices": [
            "Conservative",
            "Progressive",
            "Centrist/Moderate",
            "Libertarian",
            "Green/Environmental"
        ],
        "description": "The agent's political orientation cultural trait"
