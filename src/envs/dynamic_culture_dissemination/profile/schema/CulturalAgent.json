            "Traditional Local",
            "International",
            "Fast Food",
            "Vegetarian/Organic",
            "Gourmet/Experimental"
        ],
        "description": "The agent's culinary preference cultural trait"
    },
    "fashion_style": {
        "type": "str",
        "default": "Formal/Professional",
        "private": false,
        "sampling": "random",
        "choices": [
            "Formal/Professional",
            "Casual/Relaxed",
            "Athletic/Sporty",
            "Artistic/Alternative",
            "Luxury/High-fashion"
        ],
        "description": "The agent's fashion style cultural trait"
