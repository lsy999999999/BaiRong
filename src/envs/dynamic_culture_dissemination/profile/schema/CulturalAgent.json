{
    "name": {
        "type": "str",
        "default": "CulturalAgent",
        "private": false,
        "sampling": "llm",
        "description": "The agent's designated name in the multi-agent system"
    },
    "music_preference": {
        "type": "str",
        "default": "Classical",
        "private": false,
        "sampling": "random",
