    "interaction_history": {
        "type": "list",
        "default": [
            {
                "event": "contract_negotiation",
                "outcome": "successful",
                "date": "2023-10-01"
            }
        ],
        "private": true,
        "sampling": "default"
    },
    "current_policy_focus": {
        "type": "str",
        "default": "economic_growth",
        "private": false,
        "sampling": "llm",
        "description": "Current priority in policy development"
    },
    "id": {
        "type": "str",
        "default": "0",
        "private": true,
        "sampling": "default",
        "description": "Unique id of agent"
    }
