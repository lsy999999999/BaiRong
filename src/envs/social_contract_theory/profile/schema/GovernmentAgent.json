{
    "name": {
        "type": "str",
        "default": "Government Agent",
        "private": false,
        "sampling": "llm",
        "description": "The agent's role within the government structure"
    },
    "law_details": {
        "type": "str",
        "default": "Pending legislation review",
        "private": true,
        "sampling": "default"
    },
    "enforcement_status": {
        "type": "str",
        "default": "active",
        "private": false,
        "sampling": "default"
    },
    "review_status": {
        "type": "str",
        "default": "under_review",
        "private": true,
        "sampling": "default"
    },
    "government_legitimacy": {
        "type": "int",
        "default": 85,
        "private": false,
        "sampling": "random",
        "range": [
            50,
            100
        ]
